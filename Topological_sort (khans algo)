#include<bits/stdc++.h>
using namespace std;
#define ll  long long
#define pb   push_back
#define yes   cout<<"YES"<<endl
#define no   cout<<"NO"<<endl
#define all(x) (x).begin(), (x).end()
#define    gcd(a,b)      __gcd(a,b)
#define    lcm(a,b)  (a*b)/__gcd(a,b)
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

const ll mx=1e5+123; 
vector<ll>adj[mx]; 
map<ll,ll>in; 

void solve(){

   ll n,m; 
   cin>>n>>m;
   for(ll i=1; i<=m; i++){
     ll u,v; 
     cin>>u>>v; 
     adj[u].pb(v);
     in[v]++;  
   } 

   queue<ll>q; 
   for(ll i=1; i<=n; i++){
     if(in[i]==0)q.push(i); 
   }

   vector<ll>ans; 
   while(!q.empty())
   {
     ll node=q.front(); 
     q.pop(); 
     ans.pb(node); 
     for(auto it:adj[node]){
        in[it]--; 
        if(in[it]==0)q.push(it); 
     }
   }

   if(ans.size()!=n)cout<<"IMPOSSIBLE"<<endl;//contain cycle
   else{
     for(auto i:ans)cout<<i<<" "; 
     cout<<endl;
   }


}

int main()
{
    optimize();
   
        solve();
    
}
