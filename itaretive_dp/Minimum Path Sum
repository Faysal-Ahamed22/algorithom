#include <bits/stdc++.h> 
int minSumPath(vector<vector<int>> &grid) {
   
   int n=grid.size(); 
   int m=grid[0].size(); 
   vector<vector<int>>dp(n,vector<int>(m,1e9)); 
  

   for(int i=0; i<n; i++)
   {
      for(int j=0; j<m; j++)
      {
         if(i==0 && j==0)dp[i][j]=grid[i][j]; 
          else
          {
             int val1=1e9,val2=1e9; 
             if(i>0)val1=dp[i-1][j]; 
             if(j>0)val2=dp[i][j-1]; 
             dp[i][j]=grid[i][j]+min(val1,val2); 
          }
           
      }
   } 

   return dp[n-1][m-1]; 


}
//https://www.naukri.com/code360/problems/minimum-path-sum_985349
