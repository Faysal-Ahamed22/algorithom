#include <bits/stdc++.h> 
int frogJump(int n, vector<int> &heights)
{
    vector<int>v=heights; 
    v.insert(v.begin(),0); 

    vector<int>dp(n+2,100000000);
    dp[1]=0;  
    for(int i=2; i<=n; i++){
        dp[i]=min({dp[i],dp[i-1]+abs(v[i]-v[i-1]),dp[i-2]+abs(v[i]-v[i-2])});
    }
    return dp[n]; 
}
