#include <bits/stdc++.h> 
int uniquePaths(int m, int n) {
	
	int dp[m][n]; 
    dp[0][0]=1;
	for(int i=0; i<m; i++)
	{
		for(int j=0; j<n; j++)
		{
			if(i==0 && j==0)continue; 
				int w1=0, w2=0; 
		     	if(i>0)w1=dp[i-1][j]; 
			   if(j>0)w2=dp[i][j-1]; 
			   dp[i][j]=w1+w2; 
				
			
		}
	}

	return dp[m-1][n-1]; 
} 

//https://www.naukri.com/code360/problems/total-unique-paths_1081470
