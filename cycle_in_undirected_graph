const ll mx=2e5+123; 
vector<ll>adj[mx];
ll vis[mx],parents[mx];

bool dfs( ll node )
{
    vis[node]=1; 
    parents[node]=1; 
    for(auto u:adj[node])
    {
        if(!vis[u]){
            if(dfs(u))return 1; 
        } 
              else if(parents[u])return 1; 
          }
    parents[node]=0; 
    return 0; 
}
